    "barmu_inf = np.random.multivariate_normal(75 * np.ones(maxn), 100 * np.eye(maxn))\n",
    "mV_cor = 450 * np.ones((maxn,maxn)) + np.diag(np.arange(maxn)+1)\n",
    "nvalues_fre, nsurprises_fre, nregrets_fre = sample_mu(barmu = barmu_inf, mM = 10 * np.eye(maxn), mV = mV_cor )\n",
