    "barmu_inf = np.random.multivariate_normal(75 * np.ones(maxn), 100 * np.eye(maxn))\n",
    "nvalues_fre, nsurprises_fre, nregrets_fre = sample_mu(barmu = barmu_inf, mM = 10 * np.eye(maxn), mV = 500 * np.eye(maxn))\n",
    "nvalues_bay, nsurprises_bay, nregrets_bay = sample_mu_bay(barmu = barmu_inf, mM = 10 * np.eye(maxn), mV = 500 * np.eye(maxn))"
